{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
	"author": "Nusiq",
	"name": "system-template-2",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"filter_tester": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.0.0"
			},
			"system_template_2": {
				"runWith": "deno",
				"script": "../main.ts"
			}
		},
		"formatVersion": "1.4.0",
		"profiles": {
			"default": {
				"export": {
					"target": "local"
				},
				"filters": [
					{
						"filter": "system_template_2"
					},
					{
						"filter": "filter_tester",
						"settings": {
							"errors_stop_execution": false
						}
					}
				]
			}
		}
	}
}
